{
  "name": "tax",
  "version": "1.0.0",
  "description": "Tax Calculator",
  "main": "index.js",
  "scripts": {
    "dev:server": "webpack --config webpack.config.server.js --watch",
    "dev:client": "webpack --config webpack.config.client.js --watch",
    "dev:start": "nodemon --watch devBundling --exec \"node dist/server.bundle.dev.js\"",
    "devBundling": "npm-run-all --parallel dev:*",
    "build:dev": "webpack --config webpackconfig/webpack.dev.client.js  --progress --profile --colors && webpack --config webpackconfig/webpack.dev.server.js  --progress --profile --colors",
    "build:prod": "webpack --config webpackconfig/webpack.prod.client.js  --progress --profile --colors && webpack --config webpackconfig/webpack.prod.server.js  --progress --profile --colors",
    "start": "nodemon --watch server --exec babel-node -- dist/server.bundle.dev.js",
    "build:sw": "webpack --config webpackconfig/webpack.config.sw.js"
  },
  "author": "Kushagra Varshney",
  "license": "ISC",
  "dependencies": {
    "axios": "0.19.2",
    "babel-eslint": "10.0.3",
    "babel-plugin-import-inspector": "2.0.0",
    "babel-plugin-universal-import": "1.4.0",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "connect-timeout": "1.7.0",
    "ejs": "2.5.7",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "express-useragent": "1.0.12",
    "morgan": "1.9.1",
    "nodemon": "~1.19.1",
    "prop-types": "15.7.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-loadable": "5.3.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.2.2",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "3.4.1"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "compression-webpack-plugin": "1.1.11",
    "file-loader": "2.0.0",
    "npm-run-all": "4.1.2",
    "webpack": "4.17.2",
    "webpack-cli": "3.1.1",
    "webpack-file-changer": "2.0.1",
    "webpack-merge": "4.1.2"
  }
}
