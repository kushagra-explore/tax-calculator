  "scripts": {
    "dev:server": "webpack --config webpack.config.server.js --watch",
    "dev:client": "webpack --config webpack.config.client.js --watch",
    "dev:start": "nodemon --watch devBundling --exec \"node dist/server.bundle.dev.js\"",
    "devBundling": "npm-run-all --parallel dev:*",
    "build:dev": "webpack --config webpackconfig/webpack.dev.client.js  --progress --profile --colors && webpack --config webpackconfig/webpack.dev.server.js  --progress --profile --colors",
    "build:prod": "webpack --config webpackconfig/webpack.prod.client.js  --progress --profile --colors && webpack --config webpackconfig/webpack.prod.server.js  --progress --profile --colors",
    "start": "nodemon --watch server --exec babel-node -- dist/server.bundle.dev.js",
    "build:sw": "webpack --config webpackconfig/webpack.config.sw.js"
  },
