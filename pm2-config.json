{
  "apps" : [{
    "name"        : "Tax",
    "script"      : "./dist/server.bundle.dev.js",
    "watch"       : false,
    "instances"  : 3,
    "exec_mode"  : "cluster",
    "max_memory_restart" : "1G",
    "cwd":"~/tax/",
    "log_type"  : "json",
    "error_file" : "/var/logs/node_logs/pwa_mob/pm2_error.log",
    "out_file" : "/var/logs/node_logs/pwa_mob/pm2_out.log",
    "merge_logs": true,
    "listen_timeout" : "10000",
    "max_restarts" : "10",
    "log_date_format" : "DD-MM HH:mm:ss.SSS"
  }]
}
